[MD]
steps                                        16000 ! for total 40ps
!random_seed                                       1
temperature                                     300
!initial_temperature                             0.1
stepsize                                        1.0 ! 1ps
bath_coupling                                    10
separate_scaling                                 on
lrf                                              on
shake_all_hydrogens                              on
!shake_solvent                                   on

[cut-offs]
solute_solute                                    10
solvent_solvent                                  10
solute_solvent                                   10
!q_atom                                           99

[sphere]
shell_radius                                   20.0
shell_force                                    0.01 !Paper

[solvent]
!radial_force                                    60.0
polarisation                                     on
polarisation_force                             20.0 

[intervals]
non_bond                                         25
output                                           50

[files]
topology                            complexdiz_w.top 
restart                                    eqoxo2.re 
final                                      eqdiz3.re 
fep                                         diz.fep 

![sequence_restraints] ! there are restrainsts with the sphere
!1 70 0.5 0 1
